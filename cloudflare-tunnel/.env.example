# Cloudflare Tunnel Environment Variables
# Copie para .env e ajuste os valores

# Tunnel ID (obtido após executar: cloudflared tunnel create synexa-tunnel)
TUNNEL_ID=xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx

# Cloudflare Account ID (opcional, para automação)
CLOUDFLARE_ACCOUNT_ID=your-account-id

# Cloudflare Zone ID para synexa.dev (opcional)
CLOUDFLARE_ZONE_ID=your-zone-id

# Caminhos locais
BACKEND_URL=http://localhost:3000
FRONTEND_URL=http://localhost:3001

# Domínios públicos
PUBLIC_BACKEND_URL=https://backend.synexa.dev
PUBLIC_FRONTEND_URL=https://frontend.synexa.dev

# Configuração de desenvolvimento
NODE_ENV=development
DEBUG_TUNNEL=false